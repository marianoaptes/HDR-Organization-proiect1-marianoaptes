      <xs:element name="id" type="xs:attribute"/>
      <xs:element name="adresa" type="xs:string"/>
      <xs:element name="nume" type="xs:string"/>
      <xs:element name="prenume" type="xs:string"/>
      <xs:element name="varsta" type="xs:string"/>
      <xs:element name="culoarePreferata" type="xs:string"/>
    </xs:sequence>
  </xs:complexType>
</xs:element>


<xs:element name="adresa">
