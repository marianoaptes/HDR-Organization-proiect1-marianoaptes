<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE persoane [
<!ELEMENT persoane (persoana+)>
   <!ELEMENT persoana (nume,prenume,varsta,adresa,culoarePreferata)>
   <!ATTLIST persoana id CDATA "0">
   <!ELEMENT adresa (strada,numar,localitate,judet,tara)>
   <!ELEMENT nume (#PCDATA)>
   <!ELEMENT prenume (#PCDATA)>
   <!ELEMENT varsta (#PCDATA)>
   <!ELEMENT culoarePreferata (#PCDATA)>
   <!ELEMENT strada (#PCDATA)>
   <!ELEMENT numar (#PCDATA)>
   <!ELEMENT localitate (#PCDATA)>
   <!ELEMENT judet (#PCDATA)>
   <!ELEMENT tara (#PCDATA)>
]>

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
<xs:element name="persoane">
  <xs:complexType>
    <xs:sequence>
      <xs:element name="persoana" type="xs:string"/>
    </xs:sequence>
</xs:complexType>
</xs:element>

<xs:element name="persoana">
